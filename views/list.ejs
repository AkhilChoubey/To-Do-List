<%- include("header") -%>
  <div class="box" id="heading">
 <h1><%=listTitle%></h1>
  </div>
   <div class="box">
        <% for(var i = 0;i<listItem.length;i++){ %>
          <div class="item">
            <input type="checkbox">
            <p><%=listItem[i]%></p>
          </div>
       
            <% } %>
    
    <form action="/" class="item" method="post">
    <input type="text" name="newItem" autocomplete="off" placeholder="Write your new item.">
    <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
   </div>
<%- include("footer") -%>